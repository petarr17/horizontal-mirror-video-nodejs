{
  "name": "backend",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && esbuild ./src/index.ts --bundle --minify --sourcemap --platform=node --target=node16.14 --outdir=dist",
    "dev": "nodenv -E .env src/index.ts --exec \"ts-node-dev --files\"",
    "start-dev": "nodenv -E .env dist/index.js",
    "start-prod": "node dist/index.js",
    "lint": "eslint --ext .js,.ts .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "test": "jest"
  },
  "dependencies": {
    "@aws-sdk/client-mediaconvert": "^3.577.0",
    "@aws-sdk/client-s3": "^3.525.0",
    "@aws-sdk/client-sqs": "^3.556.0",
    "@aws-sdk/s3-request-presigner": "^3.525.0",
    "@fastify/autoload": "^5.8.0",
    "@fastify/awilix": "5.1.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.1.0",
    "awilix": "10.0.1",
    "aws-sdk": "^2.1572.0",
    "close-with-grace": "^1.3.0",
    "cors": "2.8.5",
    "fastify": "4.26.1",
    "fastify-sse": "^1.0.0",
    "http-errors-enhanced-cjs": "^2.0.1",
    "json-schema": "^0.4.0",
    "json-schema-to-ts": "^3.0.1",
    "lodash": "4.17.21",
    "sqs-consumer": "^9.1.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@types/eventsource": "^1.1.15",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "7.1.0",
    "@typescript-eslint/parser": "7.1.0",
    "esbuild": "0.20.1",
    "eslint": "8.57.0",
    "eventsource": "^2.0.2",
    "jest": "^29.7.0",
    "node-env-run": "^4.0.2",
    "prettier": "3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "2.0.0",
    "typescript": "5.3.3"
  }
}
